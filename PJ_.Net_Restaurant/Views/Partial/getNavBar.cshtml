@model IEnumerable<PJ_.Net_Restaurant.Models.Navbar>

@foreach (var i in Model)
            {
                <li class="navbar-item">
                    <a href="@i.link" class="navbar-link hover-underline">
                        <div class="separator"></div>

                        <span class="span">@i.name</span>
                    </a>
                </li>
            }
